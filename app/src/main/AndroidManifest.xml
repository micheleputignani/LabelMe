<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="src.com.labelme">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true">
        <activity
            android:name=".core.LoginActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".core.SignupActivity" />
        <activity android:name=".core.MainActivity" android:theme="@style/MyMaterialTheme.Base" />
        <activity android:name=".model.DetailsActivity" android:theme="@style/MyMaterialTheme.Base" />
    </application>

</manifest>
